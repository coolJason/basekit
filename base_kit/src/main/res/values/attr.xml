<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GridRecyclerView">
        <attr name="spanCount" format="integer" />
        <attr name="horizontal_margin" format="dimension" />
        <attr name="vertical_margin" format="dimension" />
        <attr name="canScrollVertically" format="boolean" />
        <attr name="canScrollHorizontally" format="boolean" />
    </declare-styleable>

    <!--涟漪控件属性-->
    <declare-styleable name="RippleView">
        <attr name="cColor" format="color"/>
        <attr name="cSpeed" format="float"/>
        <attr name="cDensity" format="integer"/>
        <attr name="cIsFill" format="boolean"/>
        <attr name="cIsAlpha" format="boolean"/>
        <attr name="cFromRadius" format="reference"/>
        <attr name="cToRadius" format="reference"/>
    </declare-styleable>
</resources>